  <h2>Open Console to See the Output</h2>
  <script>
    let singleQuote = 'This is a string using single quotes';
    let doubleQuote = "This is a string using double quotes";
    let backtick = `This is a string using backticks`;
    console.log(singleQuote);
    console.log(doubleQuote);
    console.log(backtick);
  </script>
</body>
