class Animal {
    constructor(name) {
        this.name = name;
    }
    speak() {
        console.log(`${this.name} makes a noise.`);
    }
}
class Dog extends Animal {
    constructor(name, breed) {
        super(name); 
        this.breed = breed;
    }
    speak() {
        console.log(`${this.name} barks.`); 
    }
}
class GoldenRetriever extends Dog {
    constructor(name, breed, color) {
        super(name, breed); 
        this.color = color;
    }
    fetch() {
        console.log(`${this.name} is fetching the ball.`);
    }
}
const myPet = new GoldenRetriever('Buddy', 'Golden Retriever', 'Golden');
myPet.speak(); 
myPet.fetch(); 
console.log(`${myPet.name} is a ${myPet.color} ${myPet.breed}.`);

