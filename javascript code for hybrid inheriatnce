class Animal {
    constructor(name) {
        this.name = name;
    }
    speak() {
        console.log(`${this.name} makes a noise.`);
    }
}
class Dog extends Animal {
    speak() {
        console.log(`${this.name} barks.`);
    }
}
const TrainerMixin = {
    train() {
        console.log(`${this.name} is being trained.`);
    }
};
class Puppy extends Dog {
}
Object.assign(Puppy.prototype, TrainerMixin);
const myPuppy = new Puppy("Buddy");
myPuppy.speak(); 
myPuppy.train(); 
